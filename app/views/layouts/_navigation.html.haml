.contain-to-grid
  %nav.top-bar{"data-topbar" => ""}
    %ul.title-area
      %li.name= link_to 'Shelter Finder', root_path
      %li.toggle-topbar.menu-icon
        %a{href: "#"}
          %span Menu
    .top-bar-section
      %ul
        - if user_signed_in?
          %li=link_to('Logout', destroy_user_session_path)
        - else
          %li=link_to('Login', new_user_session_path)
        %li= link_to 'Register an organization!', new_user_registration_path
